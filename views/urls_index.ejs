  <title>TinyApp</title>
</head>
<body style="font-size:20px; padding:50px;">
    <!-- Add EJS code here -->
    <h1>TinnyApp</h1>
    <h3>My URLS</h3>
    <a href="https://www.google.com/">Create New TinyURL</a>
    <div>
        <ul>
            <% for(url in urls) { %>
                <li><%= url + ": " + urls[url] %></li>
           <% } %>
        </ul>
